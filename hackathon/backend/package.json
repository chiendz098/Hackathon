{
  "name": "fpt-university-backend",
  "version": "1.0.0",
  "description": "Backend API for FPT COMPASS Learning Platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:with-chatbot": "node start-with-chatbot.js",
    "dev": "nodemon server.js",
    "dev:with-chatbot": "nodemon start-with-chatbot.js",
    "dev:fast": "nodemon start-fast.js",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "seed:supabase": "node run-seed.js",
    "db:reset": "node reset-db.js",
    "db:debug-core": "node debug-core-columns.js",
    "db:fix-core": "node scripts/run-fix-core-schema.js",
    "test": "jest",
    "chatbot:start": "cd ../chatbot_final && python app.py",
    "chatbot:install": "cd ../chatbot_final && pip install -r requirements.txt"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.54.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "puppeteer": "^24.16.2",
    "sequelize": "^6.35.2",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "education",
    "learning",
    "api",
    "supabase",
    "postgresql",
    "express",
    "socket.io"
  ],
  "author": "FPT COMPASS",
  "license": "MIT"
